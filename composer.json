{
    "name": "dariassoft/laravel-sortable",
    "description": "Sortable behavior package for Laravel",
    "keywords": [
        "laravel",
        "sort",
        "sortable",
        "sorting",
        "model",
        "view",
        "blade",
        "column"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Duli√ßi",
            "email": "info@akaunting.com",
            "homepage": "https://akaunting.com",
            "role": "Developer"
        },
        {
            "name": "Daniel Esteban Arias (fork maintainer)",
            "email": "dariassoft@gmail.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.5|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Akaunting\\Sortable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Akaunting\\Sortable\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Akaunting\\Sortable\\Provider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
